{
  "name": "mimo-magic-starter",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "vite build && tsc --project tsconfig.server.json",
    "start": "tsx watch scripts/dev.ts --clear-screen=false",
    "dev": "npm run start",
    "prod": "node dist/server/index.js",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "clean": "rm -rf dist node_modules/.vite",
    "fresh": "npm run clean && npm install && npm run build",
    "server": "tsx watch server/index.ts",
    "client": "vite --config vite.config.js",
    "kill-ports": "pkill -f 'node.*vite' || pkill -f 'node.*dev' || echo 'No processes found'"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-toggle": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.8",
    "better-sqlite3": "^11.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.1",
    "express": "^5.1.0",
    "kysely": "^0.28.2",
    "lucide-react": "^0.474.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.3.1"
  },
  "overrides": {
    "micromatch": ">=4.0.8",
    "braces": ">=3.0.3"
  }
}